{
  "expo": {
    "name": "BookVerse",
    "slug": "BookVerse",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#001F3D"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#001F3D"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
